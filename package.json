    "@vitest/coverage-v8": "2.1.2",
    "aws-lambda": "^1.0.7",
    "dynamo-db-local": "^9.2.1",
    "dynamodb-onetable": "^2.7.5",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vitest": "^2.1.2",
    "vitest-mock-extended": "^2.0.2",
    "wait-port": "^1.1.0",
